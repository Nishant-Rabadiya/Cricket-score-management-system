{
  "compilerOptions": {
    "moduleResolution": "node",
    
    "jsx": "react",

    "target": "esnext",

    "module": "esnext",      

    "lib": ["esnext", "dom"],

    "esModuleInterop": true,  

    "resolveJsonModule": true,                   

    "forceConsistentCasingInFileNames": true,          

    "strict": true,

    "skipLibCheck": true
  }
}